	"devDependencies": {
		"semistandard": "^8.0.0"
	},
	"scripts": {
		"debug": "DEBUG=* homebridge --debug --plugin-path .",
		"test": "./node_modules/.bin/semistandard",
		"preversion": "npm test",
		"postversion": "git push && git push --tags"
	}
